  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@premiere_lettre">
            <Value>=Parameters!premiere_lettre.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Title, FirstName, LastName, ModifiedDate
FROM     Person.Person
WHERE LastName LIKE @premiere_lettre + '%'</CommandText>
      </Query>
      <Fields>
        <Field Name="Titre">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
