        <Field Name="Modifié_le">
          <DataField>ModifiedDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsPremiereLettre">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT LEFT(LastName, 1) AS PremiereLettre
FROM Person.Person
GROUP BY LEFT(LastName, 1) ORDER BY PremiereLettre</CommandText>
      </Query>
      <Fields>
        <Field Name="PremiereLettre">
          <DataField>PremiereLettre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
